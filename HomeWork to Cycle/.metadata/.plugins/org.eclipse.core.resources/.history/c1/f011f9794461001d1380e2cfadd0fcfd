package task12;
import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter a: ");
		float a = sc.nextDouble();
		System.out.print("Enter n: ");
		int n = sc.nextInt();
		
		System.out.printf("Result: %s ",Math.pow(a, n));
		
	}

}